<table style="width: 100%;">
    <tr>
        <th>Name</th>
        <th>Total BP</th>
        <th>Coverage</th>
        <th>Read Total</th>
        <th>Read min</th>
        <th>Read mean</th>
        <th>Read std</th>
        <th>Read median</th>
        <th>Read max</th>
        <th>Read 25th</th>
        <th>Read 75th</th>
        <th>Quality min</th>
        <th>Quality mean</th>
        <th>Quality std</th>
        <th>Quality max</th>
        <th>Quality median</th>
        <th>Quality 25th</th>
        <th>Quality 75th</th>
    </tr>
    <% sequence_summary.forEach(function (summary) { %>
    <tr>
        <td > <%= summary.name %> </td>
        <td > <%= summary.total_bp %> </td>
        <td > <%= summary.coverage %> </td>
        <td > <%= summary.read_total %> </td>
        <td > <%= summary.read_min %> </td>
        <td > <%= summary.read_mean %> </td>
        <td > <%= summary.read_std %> </td>
        <td > <%= summary.read_median %> </td>
        <td > <%= summary.read_max %> </td>
        <td > <%= summary.read_25th %> </td>
        <td > <%= summary.read_75th %> </td>
        <td > <%= summary.qual_min %> </td>
        <td > <%= summary.qual_mean %> </td>
        <td > <%= summary.qual_std %> </td>
        <td > <%= summary.qual_max %> </td>
        <td > <%= summary.qual_median %> </td>
        <td > <%= summary.qual_25th %> </td>
        <td > <%= summary.qual_75th %> </td>
    </tr>

    <% }) %>
</table>
