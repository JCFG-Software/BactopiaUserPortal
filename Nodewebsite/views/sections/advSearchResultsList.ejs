<div id="queryTitleContainer">
    <h2>Advanced Search results</h2>
</div>

<div id="queryListContainer">
    <div class="list-group">
        <form action='/result' method="get">
            <% samples.forEach(function (result_samples) { %>

        <button type="submit" name="sampleSelection" value="<%= result_samples.sample%>" onclick="window.location.href='/result'" class="list-group-item list-group-item-action list-group-item-light" style="color: #0a0a0a">
            Sample: <%= result_samples.sample %>,
            Species: <%= result_samples.species %>,
            Runtype: <%= result_samples.runtype %>,
            Genome Size: <%= result_samples.genome_size %>,
            </button>
        <% }) %>
        </form>
        <br>
        <p style="text-align: center"> Displaying result(s) <%= number %> of <%= number %></p>
    </div>
</div>
